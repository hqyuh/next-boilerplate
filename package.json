{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "dotenv -e .env.production node dist/server.js",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css)\"",
    "drizzle:generate": "drizzle-kit generate --config drizzle.config.ts",
    "drizzle:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "drizzle:push": "drizzle-kit push --config drizzle.config.ts",
    "drizzle:pull": "drizzle-kit pull --config drizzle.config.ts",
    "drizzle:check": "drizzle-kit check --config drizzle.config.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.11.2",
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-query": "^5.61.4",
    "@tanstack/react-query-devtools": "^5.61.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.4",
    "drizzle-orm": "^0.36.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.460.0",
    "next": "14.2.18",
    "next-intl": "^3.25.1",
    "next-themes": "^0.4.3",
    "nextjs-toploader": "^3.7.15",
    "nodemon": "^3.1.9",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "drizzle-kit": "^0.28.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-sonarjs": "^1.0.4",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}